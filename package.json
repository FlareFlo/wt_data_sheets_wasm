{
   "author": "FlareFlo",
   "name": "wt_data_sheets_wasm",
   "version": "0.1.0",
   "scripts": {
      "build": "rimraf dist pkg && buildstamp --out.path=buildstamp.json --date.format=instant && sass --no-source-map static/scss:static/css && webpack",
      "start": "rimraf dist pkg && buildstamp --out.path=buildstamp.json --date.format=instant && webpack-dev-server --open -d",
      "start_sass": "sass --no-source-map --watch static/scss:static/css",
      "test": "cargo test && wasm-pack test --headless"
   },
   "devDependencies": {
      "@wasm-tool/wasm-pack-plugin": "^1.1.0",
      "copy-webpack-plugin": "^5.1.2",
      "rimraf": "^3.0.0",
      "webpack": "^4.46.0",
      "webpack-cli": "^3.3.3",
      "webpack-dev-server": "^3.7.1",
      "webpack-merge": "^5.8.0",
      "workbox-webpack-plugin": "^6.4.1"
   },
   "dependencies": {
      "buildstamp": "^1.4.3",
      "mathjax": "^3.2.0",
      "node-sass": "^7.0.1"
   }
}
